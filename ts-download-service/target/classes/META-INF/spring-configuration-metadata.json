{
  "groups": [
    {
      "name": "aws.s3",
      "type": "com.ts.download.config.CosProperties",
      "sourceType": "com.ts.download.config.CosProperties"
    },
    {
      "name": "local.file",
      "type": "com.ts.download.config.LocalFileProperties",
      "sourceType": "com.ts.download.config.LocalFileProperties"
    },
    {
      "name": "spring.datasource",
      "type": "javax.sql.DataSource",
      "sourceType": "com.ts.download.config.ClickHouseConfig",
      "sourceMethod": "clickHouseDataSource()"
    }
  ],
  "properties": [
    {
      "name": "aws.s3.access-key-id",
      "type": "java.lang.String",
      "description": "访问密钥ID (SecretId)",
      "sourceType": "com.ts.download.config.CosProperties"
    },
    {
      "name": "aws.s3.bucket-name",
      "type": "java.lang.String",
      "description": "存储桶名称",
      "sourceType": "com.ts.download.config.CosProperties"
    },
    {
      "name": "aws.s3.endpoint",
      "type": "java.lang.String",
      "description": "端点",
      "sourceType": "com.ts.download.config.CosProperties"
    },
    {
      "name": "aws.s3.region",
      "type": "java.lang.String",
      "description": "区域",
      "sourceType": "com.ts.download.config.CosProperties"
    },
    {
      "name": "aws.s3.secret-access-key",
      "type": "java.lang.String",
      "description": "访问密钥 (SecretKey)",
      "sourceType": "com.ts.download.config.CosProperties"
    },
    {
      "name": "local.file.access-prefix",
      "type": "java.lang.String",
      "description": "文件访问路径前缀",
      "sourceType": "com.ts.download.config.LocalFileProperties",
      "defaultValue": "\/files"
    },
    {
      "name": "local.file.base-path",
      "type": "java.lang.String",
      "description": "本地文件存储根目录",
      "sourceType": "com.ts.download.config.LocalFileProperties",
      "defaultValue": "D:\/ts-download-files"
    },
    {
      "name": "local.file.enabled",
      "type": "java.lang.Boolean",
      "description": "是否启用本地存储（true=本地存储，false=COS存储）",
      "sourceType": "com.ts.download.config.LocalFileProperties",
      "defaultValue": true
    },
    {
      "name": "local.file.server-domain",
      "type": "java.lang.String",
      "description": "服务器域名或IP",
      "sourceType": "com.ts.download.config.LocalFileProperties",
      "defaultValue": "http:\/\/localhost:8080"
    }
  ],
  "hints": []
}